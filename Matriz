int main() {
    
    int matriz[5][4] ={ 
    { 7 , 8 , 9 , 4 },
    { 5 , 10 , 1 , 0 },
    { 4 , 10 , 10 , 10 },
    { 10 , 1 , 6 , 0 },
    { 2 , 8 , 4 , 0 }
    
   };
   

   printf("imprimindo os elementos da matriz:\n");
   for (int i = 0; i < 5; i++ ) { 
       for (int j = 0; j < 4; j++){
           printf("%d ", matriz[i][j]);
           
       }
       printf("\n");
   }
   
   
    
    

    return 0;
}
